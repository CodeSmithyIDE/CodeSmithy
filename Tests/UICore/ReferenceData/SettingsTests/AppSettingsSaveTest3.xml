<?xml version="1.0"?>
<codesmithy-application-settings>
	<file-format-version>0.1.0</file-format-version>
	<file-type-associations />
	<startup-settings>
		<initial-size>
			<initial-size-type>fixed-size</initial-size-type>
			<width>800</width>
			<height>600</height>
		</initial-size>
		<startup-behavior>display-startup-page</startup-behavior>
		<startup-workspace></startup-workspace>
		<os-boot-behavior>none</os-boot-behavior>
	</startup-settings>
	<editor-settings>
		<default-editor-settings>
			<theme-name>CodeSmithy Light Theme</theme-name>
			<override-theme>false</override-theme>
			<font-settings>
				<font-face-name>Courier New</font-face-name>
				<font-point-size>10</font-point-size>
			</font-settings>
		</default-editor-settings>
		<bakefile-editor-settings>
			<use-default-settings>true</use-default-settings>
			<theme-name>CodeSmithy Light Theme</theme-name>
			<override-theme>false</override-theme>
			<font-settings>
				<font-face-name>Courier New</font-face-name>
				<font-point-size>10</font-point-size>
			</font-settings>
		</bakefile-editor-settings>
		<cmakelists-editor-settings>
			<use-default-settings>true</use-default-settings>
			<theme-name>CodeSmithy Light Theme</theme-name>
			<override-theme>false</override-theme>
			<font-settings>
				<font-face-name>Courier New</font-face-name>
				<font-point-size>10</font-point-size>
			</font-settings>
		</cmakelists-editor-settings>
		<cpp-editor-settings>
			<use-default-settings>true</use-default-settings>
			<theme-name>CodeSmithy Light Theme</theme-name>
			<override-theme>true</override-theme>
			<font-settings>
				<font-face-name>Arial</font-face-name>
				<font-point-size>12</font-point-size>
			</font-settings>
			<styles>
				<style>
					<id>comment</id>
					<text-color>0</text-color>
				</style>
			</styles>
		</cpp-editor-settings>
		<powershell-editor-settings>
			<use-default-settings>true</use-default-settings>
			<theme-name>CodeSmithy Light Theme</theme-name>
			<override-theme>false</override-theme>
			<font-settings>
				<font-face-name>Courier New</font-face-name>
				<font-point-size>10</font-point-size>
			</font-settings>
		</powershell-editor-settings>
		<python-editor-settings>
			<use-default-settings>true</use-default-settings>
			<theme-name>CodeSmithy Light Theme</theme-name>
			<override-theme>false</override-theme>
			<font-settings>
				<font-face-name>Courier New</font-face-name>
				<font-point-size>10</font-point-size>
			</font-settings>
		</python-editor-settings>
		<xml-editor-settings>
			<use-default-settings>true</use-default-settings>
			<theme-name>CodeSmithy Light Theme</theme-name>
			<override-theme>false</override-theme>
			<font-settings>
				<font-face-name>Courier New</font-face-name>
				<font-point-size>10</font-point-size>
			</font-settings>
			<styles>
				<style>
					<id>element-name</id>
					<text-color>0</text-color>
				</style>
				<style>
					<id>comment</id>
					<text-color>0</text-color>
				</style>
			</styles>
		</xml-editor-settings>
	</editor-settings>
	<tools-settings>
		<bakefile-tool-settings />
		<gitbash-tool-settings />
	</tools-settings>
	<advanced-settings>
		<ui-log-level>disabled</ui-log-level>
	</advanced-settings>
</codesmithy-application-settings>
